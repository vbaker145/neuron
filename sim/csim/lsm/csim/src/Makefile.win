#
# the default rule
#
default: version.i csim.dll

version.i: Makefile Makefile.win
	echo #define _VERSION_STRING_ $(VERSION_STRING) > version.i


#
# the *.i files
#
classlist.i switch.i registerclasses.i: *.h
	$(REGGEN) RegGenFile
	@del /S /Q def html

field-files:
	$(REGGEN) RegGenFile
	@del /S /Q def html

#
# the rule how to make an *.obj file from an *.cpp file
#
.cpp.obj:
	$(MEX) $(COMPILEOPTS) -c $<
	
#
# how to make the mex file
#
csim.dll: $(OBJECTS) csim.cpp version.i
	$(MEX) $(LINKOPTS) csim.cpp $(OBJECTS);

#
# some default targets
#
clean:
	del /Q csim.dll *.i *.obj

mrproper:
	del /Q csim.dell *.i *.obj fields-* objectlist.txt

distclean:
	del /Q *.obj fields-*

build: clean filepermissions depend csim.dll install.m

