#
# CSIM Version
#
VERSION_STRING = "1.1"

#
# Build tools
#
MEX    = "C:\Program Files\MATLAB71\bin\win32\mex.bat"
MEX    = mex.bat
REGGEN = ..\..\develop\reggen\bin\reggen.exe


#
#  compile and link options for Matlab 6.5
#
#COMPILEOPTS = -f mexopts65.bat -O
#LINKOPTS    = -D_VERSION_STRING_\#$(VERSION_STRING) -f mexopts65.bat -O

#
#  compile and link options for Matlab 7.1
#
COMPILEOPTS = -f mexopts71.bat -O
LINKOPTS    =  -output csim.dll -f mexopts71.bat -O

